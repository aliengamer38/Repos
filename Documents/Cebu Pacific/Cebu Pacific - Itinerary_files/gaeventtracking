(function(n){"use strict";n.Class.GAEventData=function(){var n=this;return n.gaEventName="userAction",n.category="",n.action="",n.label="",n.dataComplete=!1,n.onInit=function(t){t.category&&t.action&&(n.category=t.category,n.action=t.action,n.label=t.label||"",n.dataComplete=!0)},n.gaEventDataPush=function(){dataLayer&&n.dataComplete&&dataLayer.push({event:n.gaEventName,eventCategory:n.category,eventAction:n.action,eventLabel:n.label})},n}})(nca,jQuery),function(n,t){"use strict";n.Class.GAEventTrackingData=function(){var i=this;return i.eventTrackData=[],i.onReady=function(){var u,e,r,f;if(dataLayer&&i.eventTrackData.length)for(u=0,e=i.eventTrackData.length;u<e;u++)if(r=i.eventTrackData[u],r){f=n.createInstance("GAEventData");f.onInit(r);if(f.dataComplete&&r.jSEvent&&r.elementQuery)t(r.elementQuery).on(r.jSEvent,f.gaEventDataPush)}},i}}(nca,jQuery)